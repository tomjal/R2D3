{
  "name": "r2d3",
  "version": "0.0.1",
  "description": "React + Redux + D3",
  "engines": {
    "node": "6.x"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "clean": "rimraf dist/",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "dev-CMDER": "tcm -w src/styles/modules | npm run dev",
    "lint": "npm run lint-ts && npm run lint-js && npm run lint-css",
    "lint-css": "stylelint './src/**/*.css'",
    "lint-ts": "tslint 'src/**/*.tsx' 'src/**/*.ts' 'test/**/*.tsx' 'test/**/*.ts'",
    "lint-js": "eslint '**/*.js' --ignore-path .gitignore",
    "postinstall": "npm run typings && npm run build",
    "start": "cross-env NODE_ENV=production node server/node-server.js",
    "pretest": "npm run lint",
    "test": "ts-node ./node_modules/.bin/_mocha --opts ./test-utils/mocha.opts",
    "test:watch": "ts-node ./node_modules/.bin/_mocha --opts ./test-utils/mocha.opts -w",
    "typings": "rimraf typings/ && typings install"
  },
  "dependencies": {
    "axios": "^0.11.0",
    "bootstrap": "^3.3.6",
    "d3": "^4.2.3",
    "express": "^4.13.4",
    "formsy-react": "^0.18.0",
    "helmet": "^2.0.0",
    "history": "^2.0.1",
    "http-proxy": "^1.13.2",
    "i18next": "^3.1.0",
    "i18next-browser-languagedetector": "^0.3.0",
    "i18next-xhr-backend": "^0.5.4",
    "immutable": "^3.7.6",
    "moment": "^2.13.0",
    "react": "^15.0.1",
    "react-addons-pure-render-mixin": "^15.0.2",
    "react-addons-test-utils": "^15.0.1",
    "react-d3": "^0.4.0",
    "react-dom": "^15.0.1",
    "react-i18next": "^1.4.1",
    "react-loader": "^2.4.0",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.4.4",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.4.0",
    "redux-form": "^5.2.1",
    "redux-localstorage": "^0.4.0",
    "redux-logger": "^2.6.0",
    "redux-thunk": "^2.0.1",
    "victory": "^0.12.1",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "autoprefixer-core": "^6.0.1",
    "chai": "^3.5.0",
    "classnames": "^2.2.3",
    "concurrently": "^2.0.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "cssnano": "^3.5.2",
    "enzyme": "^2.2.0",
    "es5-shim": "^4.5.6",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "eslint": "^2.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^2.9.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5",
    "node-sass": "^3.7.0",
    "object-assign": "^4.0.1",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "redux-devtools": "^3.3.1",
    "require-hacker": "^2.1.3",
    "retyped-axios-tsd-ambient": "^0.8.1-0",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.0",
    "stylelint": "^6.2.0",
    "stylelint-webpack-plugin": "^0.2.0",
    "ts-loader": "^0.8.2",
    "ts-node": "^0.7.2",
    "tslint": "^3.7.4",
    "tslint-loader": "^2.1.0",
    "typed-css-modules": "^0.1.6",
    "typescript": "^1.8.10",
    "typings": "^0.8.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.9.1",
    "webpack-split-by-path": "0.0.8"
  }
}
